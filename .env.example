# Koralmbahn Canvas Environment Variables
# Copy this file to .env.development.local for local development

# API URL (local: http://localhost:8080, production: https://eventcrawler-api.arkturian.com)
VITE_KORALMBAHN_API_URL=http://localhost:8080

# Storage API URL
VITE_KORALMBAHN_STORAGE_URL=https://api-storage.arkturian.com

# Use imageproxy.php for CORS bypass (true/false)
# When true: Images load via https://share.arkturian.com/imageproxy.php?url=...
# When false: Images load directly from Storage API
VITE_USE_IMAGE_PROXY=false
