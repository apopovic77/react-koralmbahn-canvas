# Koralmbahn Canvas Production Environment
# API URL points to eventcrawler v2 on arkturian.com
VITE_KORALMBAHN_API_URL=https://eventcrawler-api.arkturian.com

# Storage API URL
VITE_KORALMBAHN_STORAGE_URL=https://api-storage.arkturian.com

# Image source mode (storage/proxy/local)
# - storage: Direct from Storage API (default)
# - proxy: Via imageproxy.php for CORS bypass
# - local: From local filesystem via events.php (offline fallback)
VITE_IMAGE_SOURCE=storage
